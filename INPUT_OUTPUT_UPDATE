#include <iostream>
using namespace std;

int main(){
    int marks[100];
    //int marks[100]={-1} make all the remaining elements as 0 whose value are not given ;
    int n;
    cout<<"enter the no of students ";
    cin>>n;

    //assign a value for 0
    marks[0]=-1; 

    //input 
    for(int i=1 ; i<=n; i++){
        cin>> marks[i];
        marks[i]=marks[i]*2;
    }

    //output 
    for(int i=0 ; i<=n; i++){
        cout<< marks[i] <<" ,";

    }

    cout <<endl;
    return 0;
}
